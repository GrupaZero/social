{
    "name": "gzero/social",
    "description": "Gzero Social Plugin",
    "keywords": ["framework", "laravel", "cms", "social", "plugin"],
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Skierniewski",
            "email": "adrian.skierniewski@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "gzero/core": "dev-master",
        "gzero/entity": "dev-master",
        "gzero/repository": "dev-master",
        "laravel/socialite": "^2.0"
    },
    "require-dev": {
        "fzaninotto/faker": "1.4.0",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "phpmd/phpmd": "2.*",
        "squizlabs/php_codesniffer": "1.*"
    },
    "autoload": {
        "psr-4": {
            "Gzero\\": "src/Gzero"
        },
        "classmap": [
            "src/migrations"
        ]
    },
    "minimum-stability": "stable"
}
