{
    "name": "gzero/social",
    "description": "Gzero Social Plugin",
    "keywords": [
        "framework",
        "laravel",
        "cms",
        "social",
        "plugin"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Skierniewski",
            "email": "adrian.skierniewski@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "gzero/core": "dev-master@dev",
        "gzero/entity": "dev-master@dev",
        "gzero/repository": "dev-master@dev",
        "laravel/socialite": "^2.0"
    },
    "require-dev": {
        "fzaninotto/faker": "1.6.0",
        "mockery/mockery": "0.9.*",
        "phpmd/phpmd": "2.*",
        "phpunit/phpunit": "~6.0",
        "satooshi/php-coveralls": "^1.0",
        "squizlabs/php_codesniffer": "2.*"
    },
    "autoload": {
        "psr-4": {
            "Gzero\\": "src/Gzero"
        },
        "classmap": [
            "database/migrations"
        ]
    },
    "minimum-stability": "stable"
}
